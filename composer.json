{
  "name": "bytic/hello",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/hello",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
